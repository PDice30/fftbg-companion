{
  "name": "fftbg-companion",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json ./server",
    "server:dev2": "nodemon --exec node --loader ts-node/esm ./server",
    "server:dev3": "tsc --project tsconfig.server.json && node ./dist/server/index.js",
    "dev-server": "tsc-watch --noClear -p ./tsconfig.server.json --onSuccess \"node ./dist/server/index.js\"",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.0"
  }
}
